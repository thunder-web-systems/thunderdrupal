{
    "name": "drupal/thunderdrupal",
    "description": "drupal da thunder",
    "type": "project",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "bower-asset/colorbox": "1.6.4",
        "npm-asset/tabletoolstoolbar": "0.0.1",
        "composer/installers": "^1.9",
        "drupal/captcha": "1.1",
        "drupal/core-composer-scaffold": "9.0.6",
        "drupal/core-recommended": "9.0.6",
        "drupal/d8w3css": "1.17",
        "drupal/google_analytics": "3.1",
        "drupal/imce": "2.3",
        "drupal/pathauto": "1.8",
        "drupal/recaptcha": "3.0",
        "drupal/smtp": "1.0",
        "drupal/token": "1.7",
        "drush/drush": "10.3",
        "drupal/colorbox": "1.6",
        "npm-asset/fullcalendar": "4.4.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
